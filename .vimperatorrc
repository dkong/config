source! ~/.vimperatorrc.local

" vim: set ft=vimperator:

" This should enable awesome bar like suppor
:set wildoptions=auto
:set cpt=l

" Disable session tracking
:set! browser.startup.page=1

"No sounds
:set vb

:hi -a StatusLine font-size: 140%;
:hi -a Normal font-size: 125%;
:hi -a TabText font-size: 120;
:hi -a CmdLine font-size: 120;
